{"ast":null,"code":"import E from 'wangeditor';\nexport default {\n  name: \"ProjectAdd\",\n\n  data() {\n    let that = this;\n    return {\n      pickerOptionsStart: {\n        disabledDate(time) {\n          let endDateVal = that.project.start;\n\n          if (endDateVal) {\n            return time.getTime() > new Date(endDateVal).getTime();\n          }\n        }\n\n      },\n      pickerOptionsEnd: {\n        disabledDate(time) {\n          let beginDateVal = that.project.start;\n\n          if (beginDateVal) {\n            return time.getTime() < new Date(beginDateVal).getTime();\n          }\n        }\n\n      },\n      categoryList: [],\n      mode: 'default',\n      // or 'simple'\n      dateRange: \"\",\n      editorContent: '',\n      editor: null,\n      typeList: [{\n        type: 0,\n        label: \"公益\"\n      }, {\n        type: 1,\n        label: \"非公益\"\n      }],\n      project: {\n        headPic: \"\",\n        caption: \"\",\n        targetPrice: \"\",\n        project_category_id: \"\",\n        start: \"\",\n        end: \"\",\n        type: \"\",\n        content: \"\"\n      }\n    };\n  },\n\n  methods: {\n    saveOrUpdate() {\n      this.project.content = this.editor.txt.html();\n      this.project.start = this.dateRange[0];\n      this.project.end = this.dateRange[1];\n      this.axios.post(\"/api/home/project/saveOrUpdate\", this.project).then(({\n        data\n      }) => {\n        if (data.flag) {\n          this.$message.info(data.message);\n        }\n      });\n    },\n\n    getCategoryList() {\n      this.axios.get(\"/api/category/categoryList\").then(({\n        data\n      }) => {\n        if (data.flag) {\n          this.categoryList = data.data;\n          this.$message.success(data.message);\n        } else {\n          this.$message.error(data.message);\n        }\n      });\n    },\n\n    uploadCover(response) {\n      console.log(response.data);\n      this.project.headPic = response.data;\n    }\n\n  },\n\n  created() {\n    this.getCategoryList();\n  },\n\n  mounted() {\n    this.editor = new E(this.$refs.editor);\n    this.editor.customConfig = this.editor.customConfig ? this.editor.customConfig : this.editor.config;\n\n    this.editor.customConfig.onchange = html => {\n      // console.log(html,'----------------',a)\n      this.editorContent = html;\n    };\n\n    this.editor.customConfig.uploadFileName = 'file'; //设置文件上传的参数名称\n\n    this.editor.customConfig.uploadImgServer = 'http://localhost:10010/file/editorImg'; // 上传图片到服务器\n    // 隐藏“网络图片”tab\n\n    this.editor.customConfig.showLinkImg = false;\n    this.editor.create();\n  }\n\n};","map":{"version":3,"mappings":"AA0EA;AACA;EACAA,kBADA;;EAGAC;IACA;IACA;MAEAC;QAEAC;UACA;;UACA;YACA;UACA;QACA;;MAPA,CAFA;MAWAC;QACAD;UACA;;UACA;YACA;UACA;QACA;;MANA,CAXA;MAoBAE,gBApBA;MAqBAC,eArBA;MAqBA;MACAC,aAtBA;MAuBAC,iBAvBA;MAwBAC,YAxBA;MAyBAC,WACA;QACAC,OADA;QAEAC;MAFA,CADA,EAKA;QACAD,OADA;QAEAC;MAFA,CALA,CAzBA;MAmCAC;QACAC,WADA;QAEAC,WAFA;QAGAC,eAHA;QAIAC,uBAJA;QAKAC,SALA;QAMAC,OANA;QAOAR,QAPA;QAQAS;MARA;IAnCA;EA8CA,CAnDA;;EAoDAC;IAEAC;MACA;MACA;MACA;MACA;QAAArB;MAAA;QACA;UACA;QACA;MACA,CAJA;IAKA,CAXA;;IAYAsB;MACA;QAAAtB;MAAA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CArBA;;IAuBAuB;MACAC;MACA;IACA;;EA1BA,CApDA;;EAiFAC;IACA;EACA,CAnFA;;EAoFAC;IAEA;IACA;;IACA;MACA;MACA;IACA,CAHA;;IAKA,iDATA,CASA;;IACA,mFAVA,CAUA;IACA;;IACA;IACA;EACA;;AAlGA","names":["name","data","pickerOptionsStart","disabledDate","pickerOptionsEnd","categoryList","mode","dateRange","editorContent","editor","typeList","type","label","project","headPic","caption","targetPrice","project_category_id","start","end","content","methods","saveOrUpdate","getCategoryList","uploadCover","console","created","mounted"],"sourceRoot":"src/views/project","sources":["ProjectAdd.vue"],"sourcesContent":["<template>\r\n  <div class=\"box-container\">\r\n    <el-form ref=\"form\"  :model=\"project\" label-width=\"80px\">\r\n      <el-form-item label=\"封面图:\">\r\n        <el-upload\r\n            class=\"upload-cover\"\r\n            drag\r\n            action=\"http://localhost:10010/file/images\"\r\n            multiple\r\n            :on-success=\"uploadCover\">\r\n          <i class=\"el-icon-upload\"></i>\r\n          <div class=\"el-upload__text\">将文件拖到此处，或<em>点击上传</em></div>\r\n        </el-upload>\r\n      </el-form-item>\r\n      <el-form-item label=\"标题:\">\r\n        <el-input v-model=\"project.caption\" placeholder=\"请输入标题\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"目标金额:\">\r\n        <el-input v-model=\"project.targetPrice\" placeholder=\"请输入募款金额\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"起止时间:\">\r\n          <el-date-picker\r\n              type=\"date\"\r\n              placeholder=\"项目开始日期\"\r\n              :picker-options=\"pickerOptionsStart\"\r\n              v-model=\"project.start\"\r\n              class=\"box-date-picker\"\r\n              style=\"width: 440px\"\r\n              value-format=\"yyyy-MM-dd HH:mm\"/>\r\n          <el-date-picker\r\n              type=\"date\"\r\n              placeholder=\"项目结束日期\"\r\n              :picker-options=\"pickerOptionsEnd\"\r\n              v-model=\"project.end\"\r\n              class=\"box-date-picker\"\r\n              style=\"width: 440px\"\r\n              value-format=\"yyyy-MM-dd HH:mm\"/>\r\n      </el-form-item>\r\n      <el-form-item label=\"项目标签:\">\r\n        <el-select v-model=\"project.project_category_id\" placeholder=\"请选择\" style=\"width: 880px\">\r\n        <el-option\r\n            v-for=\"item in categoryList\"\r\n            :key=\"item.id\"\r\n            :label=\"item.name\"\r\n            :value=\"item.id\"\r\n        >\r\n        </el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"项目类型:\">\r\n        <el-select v-model=\"project.type\" placeholder=\"请选择\" style=\"width: 880px\">\r\n          <el-option\r\n              v-for=\"item in typeList\"\r\n              :key=\"item.type\"\r\n              :label=\"item.label\"\r\n              :value=\"item.type\" >\r\n          </el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"详情:\">\r\n        <div v-model=\"project.content\" ref=\"editor\"> </div>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n    <div  style=\"text-align: right\">\r\n     <el-button type=\"danger\" @click=\"saveOrUpdate\">\r\n      发 表\r\n    </el-button>\r\n      <input type=\"button\" class=\"layui-btn layui-btn-primary\" value=\"返回\" onclick=\"javascript:history.back(-1);return false;\"/>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport E from 'wangeditor'\r\nexport default {\r\nname: \"ProjectAdd\",\r\n\r\n  data() {\r\n    let that = this;\r\n    return {\r\n\r\n      pickerOptionsStart:{\r\n\r\n        disabledDate(time) {\r\n          let endDateVal =  that.project.start;\r\n          if (endDateVal) {\r\n            return time.getTime() > new Date(endDateVal).getTime();\r\n          }\r\n        },\r\n      },\r\n      pickerOptionsEnd:{\r\n        disabledDate(time) {\r\n          let beginDateVal = that.project.start;\r\n          if (beginDateVal) {\r\n            return time.getTime() < new Date(beginDateVal).getTime();\r\n          }\r\n        },\r\n      },\r\n\r\n      categoryList: [],\r\n      mode: 'default', // or 'simple'\r\n      dateRange: \"\",\r\n      editorContent: '',\r\n      editor: null,\r\n      typeList: [\r\n        {\r\n          type: 0,\r\n          label: \"公益\"\r\n        },\r\n        {\r\n          type: 1,\r\n          label: \"非公益\"\r\n        }\r\n      ],\r\n      project: {\r\n        headPic: \"\",\r\n        caption: \"\",\r\n        targetPrice: \"\",\r\n        project_category_id: \"\",\r\n        start: \"\",\r\n        end: \"\",\r\n        type: \"\",\r\n        content: \"\",\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n\r\n    saveOrUpdate() {\r\n      this.project.content = this.editor.txt.html();\r\n      this.project.start = this.dateRange[0]\r\n      this.project.end = this.dateRange[1]\r\n      this.axios.post(\"/api/home/project/saveOrUpdate\",this.project).then(({data}) => {\r\n        if(data.flag){\r\n          this.$message.info(data.message)\r\n        }\r\n      })\r\n    },\r\n    getCategoryList() {\r\n      this.axios.get(\"/api/category/categoryList\").then(({data}) => {\r\n        if(data.flag){\r\n          this.categoryList = data.data\r\n          this.$message.success(data.message)\r\n        }else {\r\n          this.$message.error(data.message)\r\n        }\r\n      })\r\n    },\r\n\r\n    uploadCover(response) {\r\n      console.log(response.data)\r\n      this.project.headPic = response.data;\r\n    },\r\n\r\n  },\r\n  created() {\r\n    this.getCategoryList()\r\n  },\r\n  mounted() {\r\n\r\n    this.editor = new E(this.$refs.editor)\r\n    this.editor.customConfig = this.editor.customConfig ? this.editor.customConfig : this.editor.config\r\n    this.editor.customConfig.onchange = (html) => {\r\n      // console.log(html,'----------------',a)\r\n      this.editorContent = html\r\n    }\r\n\r\n    this.editor.customConfig.uploadFileName = 'file'; //设置文件上传的参数名称\r\n    this.editor.customConfig.uploadImgServer = 'http://localhost:10010/file/editorImg'  // 上传图片到服务器\r\n    // 隐藏“网络图片”tab\r\n    this.editor.customConfig.showLinkImg = false\r\n    this.editor.create()\r\n  },\r\n\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.box-container\r\n{\r\n  width: 960px;\r\n  height: auto;\r\n  margin: 15px auto;\r\n}\r\n\r\n/*.upload-cover\r\n{\r\n  width: 100px;\r\n  height: 100px;\r\n  margin-left: 4px;\r\n}*/\r\n\r\n.layui-form-label\r\n{\r\n  color: black;\r\n  width: auto;\r\n}\r\n\r\n.nice-select\r\n{\r\n  display: none;\r\n}\r\np\r\n{\r\n  color: black;\r\n}\r\n\r\ndd\r\n{\r\n  color: black;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}
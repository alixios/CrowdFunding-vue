e-column prop=\"targetPrice\" width=\"100\" label=\"目标金额\" align=\"center\" />\r\n      <!-- 发布人 -->\r\n      <el-table-column prop=\"homeUserName\" width=\"120\" label=\"发布人\" align=\"center\" />\r\n      <!-- 状态 -->\r\n      <el-table-column prop=\"status\" label=\"状态\" width=\"100\" align=\"center\" >\r\n        <template slot-scope=\"scope\" >\r\n          {{projectStatus(scope.row.status).name}}\r\n        </template>\r\n      </el-table-column>\r\n      <!-- 类型 -->\r\n      <el-table-column prop=\"type\" label=\"类型\" width=\"80\" align=\"center\" />\r\n      <el-table-column label=\"操作\" align=\"center\" width=\"200\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n              type=\"primary\"\r\n              size=\"mini\"\r\n              style=\"margin-left: 5px\"\r\n              @click=\"submit_audit(scope.row.id)\"\r\n          >\r\n            提交审核\r\n          </el-button>\r\n\r\n          <el-button\r\n              type=\"primary\"\r\n              size=\"mini\"\r\n              style=\"margin-left: 5px\"\r\n              @click=\"editProject(scope.row.id)\"\r\n          >\r\n            编辑\r\n          </el-button>\r\n\r\n          <el-button\r\n              type=\"primary\"\r\n              size=\"mini\"\r\n              style=\"margin-left: 5px\"\r\n              @click=\"editProject(scope.row.id)\"\r\n          >\r\n            添加回馈\r\n          </el-button>\r\n\r\n          <el-popconfirm\r\n              title=\"确定删除吗？\"\r\n              style=\"margin-left:10px\"\r\n              @confirm=\"updateArticleDelete(scope.row.blogId)\"\r\n              v-if=\"scope.row.isDelete == 0\"\r\n          >\r\n            <el-button size=\"mini\" type=\"danger\" slot=\"reference\">\r\n              删除\r\n            </el-button>\r\n          </el-popconfirm>\r\n          <el-popconfirm\r\n              title=\"确定恢复吗？\"\r\n              v-if=\"scope.row.isDelete == 1\"\r\n              @confirm=\"updateArticleDelete(scope.row.id)\"\
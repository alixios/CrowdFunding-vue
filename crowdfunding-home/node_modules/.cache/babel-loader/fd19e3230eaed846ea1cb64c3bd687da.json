l-icon-time\" style=\"margin-right:5px\" />\r\n          {{ scope.row.end | date }}\r\n        </template>\r\n      </el-table-column>\r\n      <!-- 目标金额  -->\r\n      <el-table-column prop=\"targetPrice\" width=\"100\" label=\"目标金额\" align=\"center\" />\r\n      <!-- 发布人 -->\r\n      <el-table-column prop=\"homeUserName\" width=\"120\" label=\"发布人\" align=\"center\" />\r\n      <!-- 状态 -->\r\n      <el-table-column prop=\"status\" label=\"状态\" width=\"100\" align=\"center\" >\r\n        <template slot-scope=\"scope\" >\r\n          {{projectStatus(scope.row.status).name}}\r\n        </template>\r\n      </el-table-column>\r\n      <!-- 类型 -->\r\n      <el-table-column prop=\"type\" label=\"类型\" width=\"80\" align=\"center\" >\r\n        <template slot-scope=\"scope\">\r\n          {{ projectType(scope.row.type).name }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" align=\"center\" width=\"200\">\r\n\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"FeedBackIndex\",\r\n  data() {\r\n    return {\r\n      dataList: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.initFeedBackList()\r\n  },\r\n  methods: {\r\n    initFeedBackList() {\r\n      const path = this.$route.path;\r\n      const arr = path.split(\"/\");\r\n      const projectId = arr[4];\r\n      if (projectId) {\r\n        console.log(projectId)\r\n        this.axios.get(`/api/home/feed_back/index/${projectId}`).then(({ data }) => {\r\n          this.dataList = data.data;\r\n        });\r\n      }\r\n    },\r\n  },\r\n  computed: {\r\n    projectStatus() {\r\n      return function (status) {\r\n        var name = \"\";\r\n        switch (status) {\r\n          case 0:\r\n            name = \"未提交\";\r\n            break;\r\n          case 1:\r\n            name = \"待审核\";\r\n            break;\r\n          case 2:\r\n            name = \"通过\";\r\n            break;\r\n          case 3:\r\n            name = \"未通过\";\r\n            break;\r\n          case 4:\r\n            name = \"募款\";\r\n            break;\r\n          case 5:\r\n            name = \"结束\";\r\n            break;\r\n        }\r\n        return {\r\n          name: name\r\n        };\r\n      };\r\n    },\r\n    projectType() {\r\n      return function (type) {\r\n        var name = \"\";\r\n        switch (type) {\r\n          case 0:\r\n            name = \"公益\";\r\n            break;\r\n          case 1:\r\n            name = \"非公益\";\r\n            break;\r\n        }\r\n        return {\r\n          name: name\r\n        };\r\n      };\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}
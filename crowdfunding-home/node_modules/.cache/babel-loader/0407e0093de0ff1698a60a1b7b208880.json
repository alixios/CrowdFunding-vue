{"ast":null,"code":"export default {\n  name: \"ProjectIndex\",\n\n  data() {\n    return {\n      dataList: [],\n      pagination: {\n        // 分页相关模型数据\n        currentPage: 1,\n        // 当前页码\n        pageSize: 6,\n        // 每页显示的记录数\n        total: 0,\n        // 总记录数\n        queryString: null // 查询条件\n\n      }\n    };\n  },\n\n  created() {\n    this.getIndexProjectList();\n  },\n\n  methods: {\n    getIndexProjectList() {\n      this.axios.post(\"/api/home/project/index\", this.pagination).then(({\n        data\n      }) => {\n        if (data.flag) {\n          this.dataList = data.data.records;\n        } else {\n          this.$message.error(data.message);\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqHA;EACAA,oBADA;;EAEAC;IACA;MACAC,YADA;MAEAC;QAAA;QACAC,cADA;QACA;QACAC,WAFA;QAEA;QACAC,QAHA;QAGA;QACAC,iBAJA,CAIA;;MAJA;IAFA;EASA,CAZA;;EAaAC;IACA;EACA,CAfA;;EAgBAC;IACAC;MACA;QAAAT;MAAA;QACA;UACA;QAEA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA;;EAVA;AAhBA","names":["name","data","dataList","pagination","currentPage","pageSize","total","queryString","created","methods","getIndexProjectList"],"sourceRoot":"src/views/project","sources":["ProjectIndex.vue"],"sourcesContent":["<template>\r\n  <div >\r\n    <!-- 表格展示 -->\r\n    <el-table\r\n        border\r\n        :data=\"dataList\"\r\n        v-loading=\"loading\"\r\n    >\r\n\r\n      <!-- 文章封面 -->\r\n      <el-table-column\r\n          prop=\"headPic\"\r\n          label=\"文章封面\"\r\n          width=\"180\"\r\n          align=\"center\"\r\n      >\r\n        <template slot-scope=\"scope\">\r\n          <el-image\r\n              class=\"article-cover\"\r\n              :src=\"\r\n              scope.row.headPic\r\n                ? scope.row.headPic\r\n                : 'https://www.static.talkxj.com/articles/c5cc2b2561bd0e3060a500198a4ad37d.png'\r\n            \"\r\n          />\r\n          <i\r\n              v-if=\"scope.row.status == 1\"\r\n              class=\"iconfont el-icon-mygongkai article-status-icon\"\r\n          />\r\n          <i\r\n              v-if=\"scope.row.status == 2\"\r\n              class=\"iconfont el-icon-mymima article-status-icon\"\r\n          />\r\n          <i\r\n              v-if=\"scope.row.status == 3\"\r\n              class=\"iconfont el-icon-mycaogaoxiang article-status-icon\"\r\n          />\r\n        </template>\r\n      </el-table-column>\r\n      <!-- 文章标题 -->\r\n      <el-table-column prop=\"caption\" label=\"标题\" align=\"center\" />\r\n      <!-- 文章分类 -->\r\n      <el-table-column\r\n          prop=\"categoryName\"\r\n          label=\"分类\"\r\n          width=\"110\"\r\n          align=\"center\"\r\n      />\r\n      <!-- 文章开始时间 -->\r\n      <el-table-column\r\n          prop=\"start\"\r\n          label=\"开始时间 \"\r\n          width=\"130\"\r\n          align=\"center\"\r\n      >\r\n        <template slot-scope=\"scope\">\r\n          <i class=\"el-icon-time\" style=\"margin-right:5px\" />\r\n          {{ scope.row.start | date }}\r\n        </template>\r\n      </el-table-column>\r\n      <!--结束时间 -->\r\n      <el-table-column prop=\"end\" label=\"结束时间\" align=\"center\" />\r\n      <!-- 目标金额  -->\r\n      <el-table-column prop=\"targetPrice\" label=\"目标金额\" align=\"center\" />\r\n      <!-- 发布人 -->\r\n      <el-table-column prop=\"homeUserName\" label=\"发布人\" align=\"center\" />\r\n      <!-- 状态 -->\r\n      <el-table-column prop=\"status\" label=\"状态\" align=\"center\" />\r\n      <!-- 类型 -->\r\n      <el-table-column prop=\"type\" label=\"类型\" align=\"center\" />\r\n      <el-table-column label=\"操作\" align=\"center\" width=\"150\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n              type=\"primary\"\r\n              size=\"mini\"\r\n              @click=\"editArticle(scope.row.blogId)\"\r\n          >\r\n            编辑\r\n          </el-button>\r\n          <el-popconfirm\r\n              title=\"确定删除吗？\"\r\n              style=\"margin-left:10px\"\r\n              @confirm=\"updateArticleDelete(scope.row.blogId)\"\r\n              v-if=\"scope.row.isDelete == 0\"\r\n          >\r\n            <el-button size=\"mini\" type=\"danger\" slot=\"reference\">\r\n              删除\r\n            </el-button>\r\n          </el-popconfirm>\r\n          <el-popconfirm\r\n              title=\"确定恢复吗？\"\r\n              v-if=\"scope.row.isDelete == 1\"\r\n              @confirm=\"updateArticleDelete(scope.row.id)\"\r\n          >\r\n            <el-button size=\"mini\" type=\"success\" slot=\"reference\">\r\n              恢复\r\n            </el-button>\r\n          </el-popconfirm>\r\n          <el-popconfirm\r\n              style=\"margin-left:10px\"\r\n              title=\"确定彻底删除吗？\"\r\n              @confirm=\"deleteArticles(scope.row.blogId)\"\r\n          >\r\n            <el-button size=\"mini\" type=\"danger\" slot=\"reference\">\r\n              删除\r\n            </el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  <div id=\"page\">\r\n\r\n</div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\nname: \"ProjectIndex\",\r\n  data() {\r\n    return {\r\n      dataList: [],\r\n      pagination: { // 分页相关模型数据\r\n        currentPage: 1, // 当前页码\r\n        pageSize: 6, // 每页显示的记录数\r\n        total: 0, // 总记录数\r\n        queryString: null // 查询条件\r\n      },\r\n    }\r\n  },\r\n  created() {\r\n    this.getIndexProjectList()\r\n  },\r\n  methods: {\r\n    getIndexProjectList() {\r\n      this.axios.post(\"/api/home/project/index\",this.pagination).then(({data}) => {\r\n        if(data.flag){\r\n          this.dataList = data.data.records\r\n\r\n        }else {\r\n          this.$message.error(data.message)\r\n        }\r\n      })\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}
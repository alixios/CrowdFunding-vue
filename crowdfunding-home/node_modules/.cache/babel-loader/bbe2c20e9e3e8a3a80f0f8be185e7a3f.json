{"ast":null,"code":"export default {\n  name: \"project\",\n\n  data() {\n    return {\n      projectList: [],\n      // 当前页要展示的博客分页列表数据\n      title: '标题内容',\n      typeList: [],\n      // 分类列表的数据\n      latestList: [],\n      // 最新发布的博客列表的数据\n      queryString: null,\n      current: 1,\n      size: 10,\n      total: 0,\n      blogInfo: {}\n    };\n  },\n\n  created() {\n    this.listProjects();\n  },\n\n  methods: {\n    listProjects() {\n      this.axios.get(\"/api/home/project/project\", {\n        params: {\n          currentPage: this.current,\n          pageSize: this.size,\n          queryString: this.queryString,\n          typeId: this.typeId\n        }\n      }).then(({\n        data\n      }) => {\n        this.projectList = data.data.records;\n        this.total = data.data.total;\n        this.loading = false;\n      });\n    },\n\n    handleCurrentChange(currentPage) {\n      // 设置最新的页码\n      this.pagination.currentPage = currentPage; // 重新调用findPage方法进行分页查询\n\n      this.listProjects();\n    },\n\n    handleSizeChange(newSize) {\n      this.pagination.pageSize = newSize;\n      this.listProjects();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkGA;EACAA,eADA;;EAEAC;IACA;MACAC,eADA;MACA;MACAC,aAFA;MAGAC,YAHA;MAGA;MAEAC,cALA;MAKA;MAEAC,iBAPA;MAQAC,UARA;MASAC,QATA;MAUAC,QAVA;MAWAC;IAXA;EAaA,CAhBA;;EAiBAC;IACA;EACA,CAnBA;;EAoBAC;IACAC;MACA;QACAC;UACAC,yBADA;UAEAC,mBAFA;UAGAV,6BAHA;UAIAW;QAJA;MADA,GAQAC,IARA,CAQA;QAAAjB;MAAA;QACA;QACA;QACA;MACA,CAZA;IAaA,CAfA;;IAiBAkB;MACA;MACA,0CAFA,CAGA;;MACA;IACA,CAtBA;;IAuBAC;MACA;MACA;IACA;;EA1BA;AApBA","names":["name","data","projectList","title","typeList","latestList","queryString","current","size","total","blogInfo","created","methods","listProjects","params","currentPage","pageSize","typeId","then","handleCurrentChange","handleSizeChange"],"sourceRoot":"src/views/project","sources":["project.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!--page-title-area start-->\r\n    <section class=\"page-tit